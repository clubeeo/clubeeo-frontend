{
  "extends": "@quasar/app-webpack/tsconfig-preset",
  "compilerOptions": {
    "baseUrl": ".",
    "lib": ["esnext", "dom"],
    "esModuleInterop": true,
    "paths": {
      "boot/*": ["src/boot/*"],
      "@src/*": ["src/*"],
      "@apps/*": ["src/apps/*"],
      "@components/*": ["src/components/*"],
      "@layouts/*": ["src/layouts/*"],
      "@pages/*": ["src/pages/*"],
      "@modules/*": ["src/modules/*"],
      "@stores/*": ["src/stores/*"]
    }
  }
}
